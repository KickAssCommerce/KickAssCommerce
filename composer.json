{
    "name": "kickasscommerce/kickasscommerce",
    "description": "A small proof of concept for Moltin",
    "type": "project",
    "license": "MIT",
    "repositories": [
        { "type": "vcs", "url": "git@github.com:KickAssCommerce/moltin.git" }
    ],
    "require": {
        "vlucas/phpdotenv": "*",
        "slim/slim": "*",
        "moltin/php-sdk": "*",
        "symfony/serializer": "^3.2",
        "symfony/property-access": "^3.2",

        "kickasscommerce/moltin": "dev-master",
        "symfony/console": "^3.2"
    },
    "require-dev": {
        "phpunit/phpunit": "*"
    },
    "autoload": {
        "psr-4": {
            "KickAss\\Commerce\\": "src/"
        }
    }
}
